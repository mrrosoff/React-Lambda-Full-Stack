{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "ES3",
		"outDir": "dist",
		"sourceMap": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"moduleResolution": "node",
		"strict": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"allowJs": true,
		"jsx": "react"
	},
	"exclude": ["coverage", "dist", "test"]
}
