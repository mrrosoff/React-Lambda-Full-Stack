	"description": "A backlog for all the games you'll never play",
	"repository": "https://github.com/jhgrins/Quaesta",
	"license": "MIT",
	"scripts": {
		"startFrontend": "webpack serve --config frontend/webpack.config.mjs",
		"buildFrontend": "webpack --config frontend/webpack.config.mjs",
		"startLambda": "serverless offline start --stage dev",
		"deployLambda": "serverless deploy",
		"start": "concurrently --kill-others-on-fail \"npm:startFrontend\" \"npm:startLambda\"",
		"unitTests": "mocha --require ts-node/register --require test/hooks.js --recursive",
		"test": "c8 --all --check-coverage --lines 50 --include 'server/' npm run unitTests"
	},
